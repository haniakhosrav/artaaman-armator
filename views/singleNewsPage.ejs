    <article class="myNews-myArticle">
        <header class="myNews-header">
            <h1> <%= article.title %></h1>
            <span>تاریخ:12-12-400</span>
        </header>
        
       
          <figure class="myNews-figure">
		 <img src="<%= article.img %>">
         
			<figcaption class="myNews-figureCapion">
                  <%= article.desc %>
            </figcaption>
		</figure>
        <div class="shareIcones" >
            <a > <span  class="iconify"  data-width="32" data-icon="carbon:share"></span></a>
            <a> <span class="iconify"    data-width="32"data-icon="iconoir:instagram"></span> </a>
            <a> <span class="iconify"   data-width="32"data-icon="ic:round-whatsapp"></span></a>
